	"try.catch.model.block": {
    "prefix": "trymodelex",              
    "body": [
      "try:",
	  "    ${TM_SELECTED_TEXT}",
      "    $1  # code",
	  "               ",
	  "    return model",
      "except Exception as ex:",
      "    model = self.set_inv_msg(model=model,msg=str(ex))$0"
    ],
    "description": "Typed try/except block like C#"
  },
  "try.catch.item.block": {
    "prefix": "tryitemex",              
    "body": [
      "try:",
	  "    ${TM_SELECTED_TEXT}",
      "    $1  # code",
	  "               ",
	  "    return model",
      "except Exception as ex:",
      "    model = self.set_inv_item_msg(model=model,msg=str(ex))$0"
    ],
    "description": "Typed try/except block like C#"
  }
  
}